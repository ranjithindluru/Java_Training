package com.vrs.oops;

public class Motorcycle extends Vehicle {

	public void startEngine() {
		System.out.println("Starting the motorcycle engine.");
	}

	@Override
	public String toString() {
		return "Motorcycle [licensePlate=" + licensePlate + ", make=" + make + ", model=" + model + ", isAvailable()="
				+ isAvailable() + ", getLicensePlate()=" + getLicensePlate() + ", getMake()=" + getMake()
				+ ", getModel()=" + getModel() + ", getClass()=" + getClass() + ", hashCode()=" + hashCode()
				+ ", toString()=" + super.toString() + "]";
	}

}
