package com.vrs.oops;

public class Bicycle extends Vehicle {

	// Constructor
	public Bicycle() {
		this.setLicensePlate(licensePlate);
		this.setMake(make);
		this.setModel(model);
		this.setAvailable(true);
	}

	public void ringBell() {
		System.out.println("Ring! Ring! The bicycle bell is ringing!");
	}

	@Override
	public String toString() {
		return "Bicycle [licensePlate=" + licensePlate + ", make=" + make + ", model=" + model + ", isAvailable()="
				+ isAvailable() + ", getLicensePlate()=" + getLicensePlate() + ", getMake()=" + getMake()
				+ ", getModel()=" + getModel() + ", getClass()=" + getClass() + ", hashCode()=" + hashCode()
				+ ", toString()=" + super.toString() + "]";
	}

}
